{
  "hosting": {
    "public": "public",
    "redirects": [
      {
        "source": "/",
        "destination": "/fr",
        "type" : 302
      }
    ],
    "rewrites": [
      {
        "source": "/api**",
        "function": "api"
      },
      {
        "source": "**",
        "function": "content"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**test.js"
    ]
  }
}
