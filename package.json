{
  "name": "perma-data-5",
  "version": "0.5.0",
  "description": "PERMA-DATA is a plant database, a collaborative tool, a game and a rich creative universe !",
  "scripts": {
    "start": "node ./src/index.js",
    "dev-start": "npx nodemon ./src/index.js",
    "test": "mocha \"./**/*test.*js\" --exclude \"./**/node_modules/**\" -g \"#e2e\" -i",
    "dev-test": "mocha \"./**/*test.*js\" --exclude \"./**/node_modules/**\"",
    "buildx-login": "webpack --config ./webpack.config.js --output ./public/bundles/context/login.js ./src/bundles/entrypoints/context/login/login.js",
    "buildp-home": "webpack --config ./webpack.config.js --output ./public/bundles/pages/home.js ./src/bundles/entrypoints/pages/home/home.js",
    "buildev-main": "webpack --config ./webpack.config.js --output ./public/bundles/dev/main.js ./src/bundles/entrypoints/main/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Skaant/perma-data-5.git"
  },
  "keywords": [
    "permaculture",
    "data",
    "collaboration",
    "science-fiction",
    "online game",
    "garden",
    "abstract poetry",
    "comics",
    "sustainability"
  ],
  "author": "PERMA-DATA VAULTS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Skaant/perma-data-5/issues"
  },
  "homepage": "https://github.com/Skaant/perma-data-5#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "babel-loader": "^8.0.6",
    "btoa": "^1.2.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "mocha": "^6.1.4",
    "mock-fs": "^4.10.1",
    "nodemon": "^1.19.1",
    "proxyquire": "^2.1.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "atob": "^2.1.2",
    "express": "^4.17.1",
    "mongodb": "^3.3.0-beta2",
    "pug": "^2.0.4",
    "redux": "^4.0.4"
  }
}
